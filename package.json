{
    "name": "latex",
    "version": "0.4.0",
    "displayName": "LaTeX Fania",
    "description": "LaTeX - extra stuff.",
    "publisher": "fania",
    "icon": "fania.jpg",
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://github.com/Fania/vscode-LaTeX.git"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "engines": {
        "vscode": "0.10.x"
    },
    "contributes": {
        "languages": [
            {
                "id": "latex",
                "aliases": [
                    "LaTeX",
                    "latex"
                ],
                "extensions": [
                    ".tex"
                ],
                "configuration": "./latex.configuration.json"
            },
            {
                "id": "tex",
                "aliases": [
                    "TeX",
                    "tex"
                ],
                "extensions": [
                    ".sty",
                    ".cls",
                    ".bbx",
                    ".cbx"
                ],
                "configuration": "./latex.configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "latex",
                "path": "./snippets/snippets.json"
            }
        ],
        "grammars": [
            {
                "language": "latex",
                "scopeName": "text.tex.latex",
                "path": "./syntaxes/latex.tmLanguage"
            },
            {
                "language": "tex",
                "scopeName": "text.tex",
                "path": "./syntaxes/tex.tmLanguage"
            }
        ]
    }
}